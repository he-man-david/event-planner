{
  "name": "event-planner",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "db:migrate:dry": "npx prisma migrate deploy --create-only --schema ./libs/db/src/schema.prisma",
    "db:migrate": "npx prisma migrate deploy --schema ./libs/db/src/schema.prisma",
    "db:generate:schema": "nx run db:generate:schema",
    "db:run": "npm run db:generate:schema && npm run db:migrate",
    "start:all:local": "npm run db:generate:schema && nx run db:docker && npm run db:migrate && nx run-many --target serve --projects webapp,linkpreview,backend,emailservice --parallel 4",
    "start:all-backend:prod": "npm run db:generate:schema && npm run db:migrate && nx run-many --target serve --configuration production --projects backend,emailservice --parallel 2",
    "build:all-backend:prod": "nx run-many --target build --configuration production --projects backend,emailservice --parallel 2",
    "build:backend": "nx run backend:build",
    "start:backend": "nx run --configuration production backend:serve",
    "build:emailservice": "nx run emailservice:build",
    "start:emailservice": "nx run --configuration production emailservice:serve",
    "build:linkpreview": "nx run linkpreview:build",
    "start:linkpreview": "nx run --configuration production linkpreview:serve",
    "build:webapp": "nx run webapp:build",
    "start:webapp": "nx run --configuration production webapp:serve",
    "createZips": "npm run build && rm -rf ./dist/zips && node ./scripts/createBeanstalkZips.js",
    "backend:prettier": "npx prettier --write ./apps/backend/src"
  },
  "private": true,
